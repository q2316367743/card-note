{
    "manifest_version": 3,
    "name": "卡片笔记",
    "version": "1.3.3",
    "description": "记录重要的想法、日志、备忘录或其他内容",
    "content_security_policy": {
        "script-src": "self",
        "object-src": "self"
    },
    "icons": {
        "128": "icons/128x128.png",
        "256": "icons/128x128@2x.png"
    },
    "action": {
        "default_title": "卡片笔记",
        "default_icon": "icons/128x128@2x.png"
    },
    "background": {
        "service_worker": "src/background.js",
        "type": "module"
    },
    "permissions": [
    ],
    "host_permissions": [
    ]
}
